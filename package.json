{
  "name": "@savaryna/tailwindcss-material-symbols",
  "version": "1.0.2",
  "license": "MIT",
  "author": "Alex Tofan",
  "description": "ðŸŒ€ A simple tailwindcss plugin to ease work with Google's Material Symbols font.",
  "homepage": "https://savaryna.github.io/tailwindcss-material-symbols/index.html",
  "bugs": {
    "url": "https://github.com/savaryna/tailwindcss-material-symbols/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savaryna/tailwindcss-material-symbols.git"
  },
  "keywords": [
    "tailwindcss",
    "plugin",
    "tailwindcss plugin",
    "material symbols",
    "material icons"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "conc pnpm:dev:*",
    "dev:ts": "tsup --watch",
    "dev:css": "tailwindcss -o public/index.css --watch",
    "build": "conc pnpm:build:*",
    "build:ts": "tsup",
    "build:css": "tailwindcss -o public/index.css",
    "check": "conc pnpm:check:*",
    "check:types": "tsc",
    "check:format": "prettier --check src/ *.json *.ts *.md",
    "check:exports": "attw --pack .",
    "format": "prettier --write src/ *.json *.ts *.md",
    "test": "pnpm publish  --access public --no-git-checks --dry-run"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.10",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "tailwindcss": "^3.4.10"
  }
}
